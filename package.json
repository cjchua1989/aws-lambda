{
  "name": "aws-lambda",
  "version": "2.0.0",
  "description": "Boiler Plate serverless",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose stop",
    "swagger:build": "eslint --ext .ts docs/swagger --fix && ts-node docs/swagger/generate.ts",
    "lint": "eslint --ext .ts src/ --fix",
    "dynamo:migrate": "ts-node -r dotenv/config dynamodb_migration.ts",
    "migrate": "node migrations.js up",
    "migrate:rollback": "node migrations.js down 100",
    "migrate:refresh": "npm run migrate:rollback && npm run migrate",
    "serve:app": "sls offline -c serverless-apis.yml",
    "test": "jest"
  },
  "author": "KnightRaven",
  "license": "ISC",
  "devDependencies": {
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.20",
    "@types/yargs": "^15.0.12",
    "aws-sdk": "^2.919.0",
    "case-anything": "^1.1.2",
    "chai-json-schema": "^1.5.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "pluralize": "^8.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "yaml": "^1.10.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dot-object": "^2.1.2",
    "@types/joi": "^14.3.4",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^14.14.19",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "dot-object": "^2.1.4",
    "dotenv": "^8.2.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.32",
    "mysql": "^2.18.1",
    "mysql-migrations": "^1.0.7",
    "npm": "^7.18.1",
    "predis": "^1.1.3",
    "randomstring": "^1.1.5",
    "serverless-api-compression": "^1.0.1",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-plugin-warmup": "^5.3.1",
    "typeorm": "^0.2.33",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  }
}
